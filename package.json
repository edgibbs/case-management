{
  "name": "case-management",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prettier:check": "prettier --list-different --config ./.prettierrc \"app/javascript/**/*.js?(x)\"",
    "test": "run-s test:*",
    "test:jest": "jest --env=jsdom --coverage",
    "test:rspec": "bundle exec rspec",
    "test:jest:watch": "npm run test:jest -- --watch",
    "lint:test:eslint": "eslint --format tap \"app/javascript/**/*.{js,jsx}\"",
    "lint:test:rubocop": "bundle exec rubocop --format tap",
    "lint:test:scss": "stylelint \"app/{assets,javascript}/**/*.{scss,css}\"",    
    "lint:test": "run-p --print-label lint:test:*",
    "lint:configtest": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "npm-run-all lint:configtest lint:test",
    "license-dump": "license-checker --csv",
    "clean": "rimraf public/packs*",
    "build:webpacker": "./bin/webpack",
    "start": "scripts/start.sh",
    "build": "npm-run-all --serial clean lint test build:webpacker"
  },
  "dependencies": {
    "@rails/webpacker": "^3.1.1",
    "axios": "^0.17.1",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-wood-duck": "^0.1.42",
    "sass-loader": "^6.0.6",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.5",
    "@types/jasmine": "^2.8.2",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.28",
    "@types/react-dom": "^16.0.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.24.1",
    "caniuse-lite": "^1.0.30000697",
    "coffeescript": "^2.0.2",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jasmine": "^2.9.1",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-no-unsanitized": "^2.0.1",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.2.1",
    "license-checker": "^15.0.0",
    "npm-run-all": "^4.1.1",
    "postcss-smart-import": "^0.7.6",
    "prettier": "^1.7.4",
    "typescript": "^2.6.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.9.7"
  }
}
