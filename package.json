{
  "name": "case-management",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prettier:check": "prettier --list-different --config ./.prettierrc \"app/javascript/**/*.js?(x)\"",
    "test": "jest --env=jsdom",
    "lint:test": "eslint app/javascript",
    "lint:configtest": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "npm-run-all lint:test lint:configtest",
    "clean": "rimraf public/packs*",
    "build:webpacker": "./bin/webpack",
    "start": "./bin/webpack-dev-server",
    "build": "npm-run-all --serial clean lint:configtest lint:test test build:webpacker"
  },
  "dependencies": {
    "@rails/webpacker": "^3.0.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.24.1",
    "caniuse-lite": "^1.0.30000697",
    "coffeescript": "^2.0.2",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.2.1",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.7.4",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.9.3"
  }
}
